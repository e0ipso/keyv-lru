    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-flow": "^7.0.0-beta.49",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-angular": "^7.0.1",
    "@emdaer/cli": "^3.2.3",
    "@emdaer/plugin-contributors-details-github": "^3.2.3",
    "@emdaer/plugin-import": "^3.2.3",
